<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>
<img src="https://media.giphy.com/media/B7o99rIuystY4/giphy.gif">

<% @lists.each do |list| %>
    <p><%= link_to list.name, list_path(list) %></p>
    <ul>
        <% list.items.each do |item| %>
            <li><%= item.list_item.item_quantity %> <%= item.name %></li>
        <% end %>
    </ul>
<% end %>
